WPPlugin TestPlugin
{
    Entity TestEntity
    {
        // DropOnDeactivate;

        ShortString title;
        Integer count;

        Authorization TestPlugin.PublicAccess;
    }

    Entity TestChildEntity
    {
        // DropOnDeactivate;

        ShortString child_title;
        Reference parent TestPlugin.TestEntity;
    }

    Action PublicAccess 'return true;';
}